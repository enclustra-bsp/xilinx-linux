/dts-v1/;
/include/ "zynq-7000.dtsi"
/include/ "zynq-enclustra-zx-common.dtsi"
/include/ "zynq-enclustra-qspi-parts.dtsi"

/ {
	model = "Enclustra Cosmos XZQ10 Board";
};

&uart0 {
	status = "okay";
};

&isl12022 {
	status = "okay";
};

&i2c0 {
	i2c_mux: i2c_mux@74 {
		compatible = "nxp,pca9547";
		reg = <0x74>;
		#address-cells = <1>;
		#size-cells = <0>;

		i2c_mux@0 {
			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			lm96080@2f {
				compatible = "lm80";
				reg = <0x2f>;
			};
		};
	};
};
