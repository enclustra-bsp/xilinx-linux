/ {
	aliases {
		ethernet0 = &gem0;
		ethernet1 = &gem1;
		i2c0 = &i2c_mgmt;
		i2c1 = &i2c_user;
		serial0 = &uart0;
		spi0 = &qspi;
	};
};

&gem0 {
	status = "okay";
	/delete-property/ local-mac-address;
	phy-mode = "rgmii-id";
	phy-handle = <&phy0>;
	phy0: phy@1 {
		reg = <1>;

	    	realtek,center-tap-not-short;		
	};
};

&gem1 {
	status = "okay";
	/delete-property/ local-mac-address;
	phy-mode = "gmii";
	phy-handle = <&phy1>;
    	phy1: phy@1 {
		reg = <1>;

	    	realtek,center-tap-not-short;		
	};
};

&qspi {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
	is-dual = <0>;
	num-cs = <1>;
	flash0: flash@0 {
		reg = <0x0>;
		compatible = "spansion,s25fl512s", "jedec,spi-nor";
		spi-rx-bus-width = <0x4>;
		spi-tx-bus-width = <0x4>;
		spi-max-frequency = <50000000>;
		#address-cells = <1>;
		#size-cells = <1>;
	};
};

&psgtr {
	status = "okay";
	clocks = <&ref100>, <&ref27>;
	clock-names = "ref2", "ref3";
};

&amba {
	ref100: ref100M {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <100000000>;
	};
	ref27: ref27M {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <27000000>;
	};
	i2c_mgmt: i2c@ff020000 {
		status = "okay";
		clock-frequency = <100000>;
		atsha204a@64 {
			status = "okay";
			compatible = "atmel,atsha204a", "atmel,atsha204-i2c";
			reg = <0x64>;
		};
	};
	i2c_user: i2c@ff030000 {};
};

